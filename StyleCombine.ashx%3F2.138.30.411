@font-face {font-family: 'NasalizationRg-Regular';font-style: normal;font-weight: normal;src: local('NasalizationRg-Regular'), url('/content/fonts/nasalization rg.woff') format('woff');font-display: swap;}


.tags {
    list-style: none;
    margin: 0;
    overflow: hidden;
    padding: 0;
    /*background-color: white;*/
}

    .tags li {
        float: left;
    }
    
    .tags .tag::before {
        background: #fff;
        border-radius: 10px;
        box-shadow: inset 0 1px rgba(0, 0, 0, 0.25);
        content: '';
        height: 6px;
        left: 10px;
        position: absolute;
        width: 6px;
        top: 10px;
    }

    .tags .tag {
        /*font-family: 'NasalizationRg-Regular';*/
        float: left;
        position: relative;
        background: #a00;
        height: 28px;
        line-height: 25px;
        min-width: 45px;
        padding: 0px 0px 0 23px !important;
        margin: 0 30px 10px 0;
        border-top-left-radius: 4px;
        border-bottom-left-radius: 4px;
        white-space:nowrap;
        text-overflow:ellipsis;
    }

        .tags .tag:after {
            left: 100%;
            top: 50%;
            border: solid transparent;
            border-color: rgba(255, 0, 0, 0);

            border-left-color: #a00;
            border-top-color: #a00;
            border-bottom-color: #a00;

            content: " ";
            height: 0;
            width: 0;
            position: absolute;
            pointer-events: none;
            border-width: 14px;
            margin-top: -14px;
            border-left-width: 8px;
            border-right-width: 8px;
        }





        .tags .tag.faded {
            opacity: 0.3;
        }



        .tags .tag.environmenttag {
            background-color: #444;
            color: #fff;
        }

            .tags .tag.environmenttag:after {
                border-left-color: #444;
                border-top-color: #444;
                border-bottom-color: #444;
            }


