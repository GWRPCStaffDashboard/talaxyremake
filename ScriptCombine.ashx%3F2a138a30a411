            _proto2._getTimePickerMainTemplate = function _getTimePickerMainTemplate() {
                var topRow = $( '<tr>' ),
                    middleRow = $( '<tr>' ),
                    bottomRow = $( '<tr>' );

                if ( this._isEnabled( 'h' ) ) {
                    topRow.append( $( '<td>' ).append( $( '<a>' ).attr( {
                        href: '#',
                        tabindex: '-1',
                        'title': this._options.tooltips.incrementHour
